} net_iface;

#define IF_UP         0x0001
#define IF_BROADCAST  0x0002
#define IF_PROMISC    0x0004
#define IF_LOOPBACK   0x0008
#define IF_MULTICAST  0x0010
#define IF_RUNNING    0x0020
