{
    uint32 ip;    
    uint16 flags;
    uint8  hw[6];   
} net_arp_table;

typedef struct 
{
    uint32 dest;
    uint32 gw;
    uint32 mask;
    uint16 flags;
    net_iface *iface;    
} net_route_table;


typedef struct 
{
    char name[8];
    uint32 addr;
    uint32 bcast;
    uint32 mask;
    uint16 flags;
    uint16 mtu;
    uint32 metric;
    
    uint32 stat_rx_packets;
    uint32 stat_rx_errors;
    uint32 stat_rx_dropped;
    uint32 stat_rx_overruns;

    uint32 stat_tx_packets;
    uint32 stat_tx_errors;
    uint32 stat_tx_dropped;
    uint32 stat_tx_overruns;

    void (*send)(void);
    void (*init)(void);    
