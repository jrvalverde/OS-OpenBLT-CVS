char *taskstate(task_t *task)
{
	static char ts[60];
	if(task->waiting_on){
		snprintf(ts,60,"Waiting on %s #%d \"%s\"",
				 rsrc_typename(task->waiting_on), task->waiting_on->id,
				 task->waiting_on->name);
		return ts;
	} else {
		return "Running";
	}
}

