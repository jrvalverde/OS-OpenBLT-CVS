uint32 readnum(const char *s);
a91 16
void dumponersrc(const char *num)
{
	int n;
	resnode_t *rn;

	n = readnum (num);
	rn = resource_list;
	while(rn) {
		if (rn->resource->id == n) {
			printres(rn->resource);
			break;
		}
		else rn = rn->next;
	}
}

