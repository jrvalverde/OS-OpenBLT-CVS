1.1.1.2
log
@openblt.070799
@
text
@d1 1
a1 1
# $Id: //depot/blt/Makefile#5 $
a36 9

floppy:
	./util/bootmaker boot/openblt.ini boot/openblt.boot -floppy

bfloppy: floppy
	dd if=boot/openblt.boot of=/dev/disk/floppy/raw bs=18k
	
lfloppy: floppy
	dd if=boot/openblt.boot of=/dev/fd0 bs=18k
@


